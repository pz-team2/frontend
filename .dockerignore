# Node.js dependencies (should not be copied, as they are installed inside the container)
node_modules
npm-debug.log

# Git-related files
.git
.gitignore

# Environment variables (keep secrets out of the image)
.env

# Markdown documentation files
*.md

# OS-specific or editor-specific files
.DS_Store
.vscode

# Test coverage files
coverage

